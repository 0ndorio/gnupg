#!/bin/sh

. $srcdir/defs.inc || exit 3

#info Checking armored encryption with a pipe
for i in $plain_files $data_files ; do
    ./run-gpg -ea --yes -r "$usrname2" < $i | tee x | ./run-gpg -o y --yes
    cmp $i y || error "$i: mismatch"
    ./run-gpg --yes < x > y
    cmp $i y || error "$i: mismatch"
done


msgstr "Il nome deve essere lungo almeno 5 caratteri\n"

#: g10/keygen.c:425
msgid "Email address: "
msgstr "Indirizzo di Email: "

#: g10/keygen.c:437
msgid "Not a valid email address\n"
msgstr "Non è un indirizzo di email valido\n"

#: g10/keygen.c:445
msgid "Comment: "
msgstr "Commento: "

#: g10/keygen.c:451
